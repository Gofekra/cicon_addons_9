<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!-- Sequences for Maintenance orders -->


        <record id="sequence_cmms_job_pm" model="ir.sequence">
            <field name="name">CMMS Preventive</field>
            <field name="code">cmms.job.order.preventive</field>
            <field name="prefix">P1-</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <!-- <field name="implementation">no_gap</field> -->
        </record>




        <record id="sequence_cmms_job_breakdown" model="ir.sequence">
            <field name="name">CMMS Breakdown</field>
            <field name="code">cmms.job.order.master.breakdown</field>
            <field name="prefix">B1-</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <!-- <field name="implementation">no_gap</field> -->
        </record>



        <record id="sequence_cmms_job_general" model="ir.sequence">
            <field name="name">CMMS General</field>
            <field name="code">cmms.job.order.master.general</field>
            <field name="prefix">G1-</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <!-- <field name="implementation">no_gap</field> -->
        </record>




        <record id="sequence_store_invoice" model="ir.sequence">
            <field name="name">CMMS Store Invoice</field>
            <field name="code">cmms.store.invoice</field>
            <field name="prefix">S1-</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
             <field name="implementation">no_gap</field>
        </record>



    </data>
</openerp>
